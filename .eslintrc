{
    "extends": [
        "google",
        "plugin:flowtype/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "script"
    },
    "plugins": [
      "flowtype"
    ],
    "rules": {
        "object-curly-spacing": ["error", "always"],
        "quotes": ["error", "double"],
        "flowtype/define-flow-type": 1,
        "flowtype/require-valid-file-annotation": [
            2,
            "always", {
                "annotationStyle": "line"
            }
        ],
        "padded-blocks": ["error", "always"]
    },
    "env": {
      "es6": true
    }
}  